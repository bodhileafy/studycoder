<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <id>central-dev</id>
      <username>admin</username>
      <password>...</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>My-Group</name>
          <url>http://prod.artifactory.example.com/artifactory/My-Group</url>
        </repository>
        <repository>
          <snapshots />
          <id>snapshots</id>
          <name>My-Group</name>
          <url>http://prod.artifactory.example.com/artifactory/My-Group</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>My-Group</name>
          <url>http://prod.artifactory.example.com/artifactory/My-Group</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>My-Group</name>
          <url>http://prod.artifactory.example.com/artifactory/My-Group</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory</id>
    </profile>
    <profile>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central-dev</id>
          <name>My-Dev-Group</name>
          <url>http://dev.artifactory.example.com/artifactory/My-Dev-Group</url>
        </repository>
        <repository>
          <snapshots />
          <id>snapshots-dev</id>
          <name>My-Dev-Group</name>
          <url>http://dev.artifactory.example.com/artifactory/My-Dev-Group</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central-dev</id>
          <name>My-Dev-Group</name>
          <url>http://dev.artifactory.example.com/artifactory/My-Dev-Group</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots-dev</id>
          <name>My-Dev-Group</name>
          <url>http://dev.artifactory.example.com/artifactory/My-Dev-Group</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory-dev</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory-dev</activeProfile>
  </activeProfiles>
</settings>
